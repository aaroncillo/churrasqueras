<div class="container">
  <%= simple_form_for @product do |f| %>
    <%= f.input :title, autofocus: true  %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :producto_url, as: :file, class:'form-control', class:"p-4" %>
    <%= f.submit class: "btn btn-success" %>
  <% end %>
</div>
